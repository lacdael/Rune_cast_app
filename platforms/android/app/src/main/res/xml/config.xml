<?xml version='1.0' encoding='utf-8'?>
<widget id="lacdael.runes" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Runes</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon platform="android" qualifier="ldpi" src="img/logo.png" />
    <icon platform="android" qualifier="mdpi" src="img/logo.png" />
    <icon platform="android" qualifier="hdpi" src="img/logo.png" />
    <icon platform="android" qualifier="xhdpi" src="img/logo.png" />
    <icon platform="android" qualifier="xxhdpi" src="img/logo.png" />
    <icon platform="android" qualifier="xxxhdpi" src="img/logo.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidLaunchMode" value="singleTop" />
    <preference name="android-minSdkVersion" value="12" />
</widget>
