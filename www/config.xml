<?xml version='1.0' encoding='utf-8'?>
<widget id="lacdael.runes" version="1.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Runes</name>
    <description>
        A rune casting tool, for divination.
    </description>
    <author email="c.lacdael@live.co.uk" href="http://www.wisdel.uk">
        Christian Lacdael
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
<preference name="AndroidLaunchMode" value="singleTop"/>
<preference name="android-minSdkVersion" value="26" />
<icon src="img/icon-mdpi.png" platform="android" qualifier="ldpi" />
<icon src="img/icon-mdpi.png" platform="android" qualifier="mdpi" />
<icon src="img/icon-hdpi.png" platform="android" qualifier="hdpi" />
<icon src="img/icon-xhdpi.png" platform="android" qualifier="xhdpi" />
<icon src="img/icon-xxhdpi.png" platform="android" qualifier="xxhdpi" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^7.1.4" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
</widget>
